      },
      "categoryid": "CATG1",
      "domaintype": "GroupDomain",
      "elements": [
        {
          "elementid": "ATTR8",
          "name": {
            "de": "Dauer",
            "en": "Duration"
          },
          "mandatory": true,
          "domainid": "DOMA8",
          "displayseq": 1,
          "description": {
